
<article class="card mb-2">
    <div class="card-body">
        <p>{{element.origin}}</p>
        <p>{{element.shortURL}}</p>
        <a href="/eliminar/{{element._id}}" class="btn btn-danger">Eliminar</a>
        <a href="/editar/{{element._id}}" class="btn btn-info">Editar</a>
        <button data-short_url="{{element.shortURL}}" class="btn btn-warning">Copiar</button>
    </div>
</article>
